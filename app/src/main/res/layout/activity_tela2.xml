<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="16dp"
    android:paddingRight="16dp"
    android:paddingTop="16dp"
    android:paddingBottom="16dp"
    tools:context="com.example.lieberson.exemploctivityforresult.Tela2">

    <TextView
        android:id="@+id/tela2"
        android:layout_width="109dp"
        android:layout_height="33dp"
        android:text="Tela 2"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/nomeTela2"
        android:layout_width="match_parent"
        android:layout_height="29dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tela2"
        android:layout_marginTop="28dp"
        android:text="Nome"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="73dp" />

    <TextView
        android:id="@+id/emailTela2"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/nomeTela2"
        android:layout_marginTop="17dp"
        android:text="Email"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="120dp" />

    <Button
        android:id="@+id/btnAceitaDadosTela2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/emailTela2"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="110dp"
        android:onClick="aceitaDadosTela2"
        android:text="Aceitar Dados"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="188dp" />

    <Button
        android:id="@+id/btnRejeitaDadosTela2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/btnAceitaDadosTela2"
        android:layout_alignBottom="@+id/btnAceitaDadosTela2"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:onClick="rejeitaDadosTela2"
        android:text="Rejeitar Dados"
        tools:layout_editor_absoluteX="202dp"
        tools:layout_editor_absoluteY="188dp" />
</RelativeLayout>
